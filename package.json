{
    "name": "basar",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "version": "1.0.0",
    "author": "Ahmed Rowaihi <ahmedrowaihi@gmail.com>",
    "description": "A library for detecting NSFW content and faces in images and videos using TensorFlow.js and Human.js.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ahmedrowaihi/basar"
    },
    "homepage": "https://github.com/ahmedrowaihi/basar",
    "bin": {
        "basar": "./scripts/cli.js"
    },
    "scripts": {
        "build": "bun build:worker && bun build:ts",
        "build:worker": "bun run build.ts",
        "build:ts": "bunx tsc --project tsconfig.build.json",
        "prepublishOnly": "bun run build"
    },
    "devDependencies": {
        "@tensorflow/tfjs-core": "^4.22.0",
        "@types/bun": "latest"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@tensorflow/tfjs-converter": "^4.22.0",
        "@vladmandic/human": "^3.3.5",
        "comlink": "^4.4.2"
    },
    "files": [
        "dist",
        "models",
        "scripts"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./worker-client": {
            "types": "./dist/features/worker-client.d.ts",
            "import": "./dist/features/worker-client.js",
            "require": "./dist/features/worker-client.js"
        },
        "./worker": {
            "types": "./dist/features/worker/index.d.ts",
            "import": "./dist/features/worker/index.js",
            "require": "./dist/features/worker/index.js"
        },
        "./types": {
            "types": "./dist/features/detection/types.d.ts",
            "import": "./dist/features/detection/types.js",
            "require": "./dist/features/detection/types.js"
        }
    }
}
